{
    "family": "sklearn",
    "containerDefinitions": [
        {
            "name": "sklearn",
            "image": "aws_account_id.dkr.ecr.us-east-1.amazonaws.com/sklearn",
            "cpu": 10,
            "memory": 500,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "entryPoint": [
                "/usr/sbin/apache2",
                "-D",
                "FOREGROUND"
            ],
            "essential": true
        }
    ]
}
